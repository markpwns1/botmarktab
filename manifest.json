{
    "manifest_version": 2,
    "name": "botmarktab",
    "version": "1.0",

    "browser_specific_settings": {
        "gecko": {
            "id": "botmarktab@bot.mark"
        }
    },

    "chrome_url_overrides" : {
        "newtab": "redirect.html"
    },

    "content_security_policy": "script-src: 'self' https://suggestqueries.google.com",

    "permissions": [
        "<all_urls>",
        "activeTab",
        "tabs",
        "storage",
        "webRequest",
        "cookies"
    ]

}